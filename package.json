    "@babel/preset-typescript": "^7.15.0",
    "@hotwired/stimulus": "^3.0.0",
    "@types/jest": "^27.0.2",
    "babel-jest": "^27.2.3",
    "jest": "^27.2.3",
    "mutationobserver-shim": "^0.3.7",
    "np": "^7.5.0",
    "prettier-standard": "16.4.1",
    "typescript": "^4.4.3",
    "vite": "^2.5.10"
  },
  "peerDependencies": {
    "@hotwired/stimulus": "^3.0.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest/jest-setup.ts"
    ]
  }
